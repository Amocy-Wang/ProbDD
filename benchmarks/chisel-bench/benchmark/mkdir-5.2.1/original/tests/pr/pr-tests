if test $code != 0; then
  $echo "Test 1a(_POSIX2_VERSION=199209) failed: $xx return code $code differs from expected value 0" 1>&2
  errors=`expr $errors + 1`
else
  cmp 1a.O $srcdir/0F > /dev/null 2>&1
  case $? in
    0) if test "$VERBOSE"; then $echo "passed 1a(_POSIX2_VERSION=199209)"; fi;;
    1) $echo "Test 1a(_POSIX2_VERSION=199209) failed: files 1a.O and $srcdir/0F differ" 1>&2
       (diff -c 1a.O $srcdir/0F) 2> /dev/null
       errors=`expr $errors + 1`;;
    2) $echo "Test 1a(_POSIX2_VERSION=199209) may have failed." 1>&2
       $echo The command "cmp 1a.O $srcdir/0F" failed. 1>&2
       errors=`expr $errors + 1`;;
  esac
fi
